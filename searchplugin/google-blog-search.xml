<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Google Blog Search</os:ShortName>
<os:Description>Search through blogs with Google's Blog Search</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<SearchForm>http://blogsearch.google.com/</SearchForm>
<os:Url type="text/html" method="GET" template="http://blogsearch.google.com/?q={searchTerms}" resultDomain="blogsearch.google.com">
</os:Url><os:Url type="application/opensearchdescription+xml" method="GET" template="http://mycroftproject.com/updateos.php/id0/googleblogsearch.xml" rel="self" resultDomain="mycroftproject.com">
</os:Url>
</SearchPlugin>